{
  "name": "home-task",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=development node ./src/bin/server",
    "lint": "eslint --ignore-path .gitignore . --fix",
    "build:openapi": "swagger-cli bundle openapi/src/index.yaml --outfile openapi/openapi.yaml --type yaml",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:nocover": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-openapi-validator": "^4.13.8",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "swagger-cli": "^4.0.4"
  }
}
